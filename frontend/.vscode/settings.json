{
    "editor.rulers": [
        120
    ],
    "cursor.composer.shouldAutoAcceptDiffs": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "backend": false,
        "frontend": false,
        "node_modules": true,
        "**/.mypy_cache": true,
        "**/.ruff_cache": true,
        "**/__pycache__": true,
        "**/*.pyc": true,
        "**/.env.example": false,
        "**/.docker/**": false,
        "**/.cursor/**": false,
        "**/.dockerignore": false,
        ".vscode": true
    },
    "cursor.general.globalCursorIgnoreList": [
        "**/.env",
        "**/credentials.json",
        "**/credentials.*.json",
        "**/secret.json",
        "**/secrets.json",
        "**/*.key",
        "**/*.pem",
        "**/*.pfx",
        "**/*.p12",
        "**/*.crt",
        "**/*.cer",
        "**/id_rsa",
        "**/id_dsa",
        "**/.ssh/id_*"
    ],
    "workbench.externalBrowser": "firefox",
    "workbench.editor.labelFormat": "medium"
}
